{
  "presets": [
    ["env", {
      "modules": false,
      "useBuiltIns": "entry"
    /*,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }*/
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime", "syntax-dynamic-import",
  ["import", {
    "libraryName": "muse-ui",
    "libraryDirectory": "lib",
    "camel2DashComponentName": false
  }, {
    "libraryName": "iview",
    "libraryDirectory": "src/components"
  }]
  ]
}
